{
    "family": "demo-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "demo-container",
            "image": "884168370825/demo_image:v1.0.0",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "memory": 512,
            "cpu": 256
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::884168370825:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::884168370825:role/ecsTaskExecutionRole",
    "tags": [
        {
            "key": "Name",
            "value": "demo-task"
        }
    ]
}
